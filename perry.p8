pico-8 cartridge // http://www.pico-8.com
version 18
__lua__
-- perry the pedal the game

local perry
local frame

function _init()
  perry = {
    position={64, 64},
    moveSpeed=2,
    jumpSpeed=10,
    jumpDuration=10,
    direction={0, 0},
    impulse={
      frame=-1000,
      start={0, 0},
      direction={0, 0},
      speed=0
    }
  }
  frame = 0
end

function _update()
  frame += 1
  local direction = {0, 0}

  if btn(0) then
    direction[1] -= 1
  end

  if btn(1) then
    direction[2] += 1
  end

  if btn(2) then
    direction[2] -= 1
  end

  if btn(3) then
    direction[2] += 1
  end

  perry.direction = direction

  local isJumping = perry.impulse.frame + perry.jumpDuration > frame

  if (btnp(4) and not isJumping) then
    direction[2] = 1
    perry.impulse = {
      frame=frame,
      start=perry.position,
      direction=direction,
      speed=perry.jumpSpeed
    }
    return
  end

  if btn(0) then
    perry.position[1] -= perry.moveSpeed
  end

  if btn(1) then
    perry.position[1] += perry.moveSpeed
  end

  if isJumping then
    local progress = (
      perry.impulse.frame + perry.jumpDuration - frame
    ) / perry.jumpDuration
  end

end

function _draw()
  cls()
  map(0, 0, 0, 0)

  local progress = (
    perry.impulse.frame + perry.jumpDuration - frame
  ) / perry.jumpDuration
  local i = perry.impulse
  print(progress, perry.position[1], perry.position[2] - 6)
  spr(mod(frame / 8, 2), perry.position[1], perry.position[2])
end


function mod(x, base)
  return x - flr(x / base) * base
end

